<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="huiiuh.com.driverfly">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity">
        </activity>
        <activity android:name=".Activity.CarTypeActivity">
        </activity>
        <activity android:name=".Activity.TestActivity">
        </activity>
        <activity android:name=".Activity.ResultActivity">
        </activity>
        <activity android:name=".Activity.IntroduceActivity">
        </activity>
        <activity android:name=".Activity.GradeActivity">
        </activity>
        <activity android:name=".Activity.URlActivity">
        </activity>
    </application>

</manifest>